<table class="db-table">
  <thead>
    <tr>
      <th>Email</th>
      <th>Password</th>
      <th>Last Login</th>
      <th>isAdmin</th>
    </tr>
  </thead>
  <tbody>
    {{#each rows}}
      <tr class="record">
        <td>{{email}}</td>
        <td>{{password}}</td>
        <td>{{lastlogin}}</td>
        <td class="is-admin-{{isadmin}}">{{isadmin}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>